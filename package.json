{
  "name": "mcp-server-gpt-image",
  "version": "1.1.0",
  "description": "MCP server for OpenAI GPT Image-1 with real-time streaming, intelligent caching, and automatic image optimization",
  "main": "dist/index.js",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pavelsukhachev/mcp-server-gpt-image.git"
  },
  "homepage": "https://github.com/pavelsukhachev/mcp-server-gpt-image#readme",
  "bugs": {
    "url": "https://github.com/pavelsukhachev/mcp-server-gpt-image/issues"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsx watch src/index.ts",
    "start": "node dist/index.js",
    "clean": "rimraf dist",
    "typecheck": "tsc --noEmit",
    "inspector": "mcp-inspector node dist/index.js",
    "test:client": "tsx examples/test-client.ts",
    "start:http": "node dist/index.js http"
  },
  "keywords": [
    "mcp",
    "openai",
    "gpt-image-1",
    "image-generation",
    "model-context-protocol",
    "streaming",
    "sse",
    "image-optimization",
    "caching",
    "server-sent-events"
  ],
  "author": "Pavel Sukhachev",
  "license": "MIT",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.0.6",
    "@types/sharp": "^0.31.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.21.1",
    "openai": "^4.76.5",
    "sharp": "^0.34.3",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@modelcontextprotocol/inspector": "^0.2.1",
    "@types/cors": "^2.8.18",
    "@types/express": "^5.0.0",
    "@types/node": "^22.10.5",
    "rimraf": "^6.0.1",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3"
  }
}
